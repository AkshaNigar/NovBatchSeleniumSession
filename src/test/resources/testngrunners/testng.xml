<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestNG Regression Suite" verbose="4" thread-count="3" parallel="tests" >

 <test name="HomePageOpenCartTest Test">
 <parameter name="url" value="https://naveenautomationlabs.com/opencart/index.php?route=common/home" />
 <parameter name="browser" value="chrome" />
 
  <classes>
   <class name="testngSessions.HomePageOpenCartTest" />
  </classes>
 </test>

 <test name="RegisterPageOpenCark Test">
   <parameter name="url" value="https://naveenautomationlabs.com/opencart/index.php?route=account/register" />
   <parameter name="browser" value="edge" />
  <classes>
   <class name="testngSessions.RegisterPageOpenCarkTest" />
  </classes>
 </test>

 <test name="LogInOpenCart Test">
  <parameter name="url" value="https://naveenautomationlabs.com/opencart/index.php?route=account/login" />
  <parameter name="browser" value="chrome" />
  <classes>
   <class name="testngSessions.LogInOpenCartTest" />
  </classes>
 </test>


</suite>